	<interface>
		<!-- Observed state values -->
		<field id="state" type="string" value="none"/>
        <field id="contentIndex" type="integer" value="1"/>
        <field id="licenseStatus" type="string"/>

        <!-- Attributes -->
        <field id="mute" type="boolean" value="false"/>
